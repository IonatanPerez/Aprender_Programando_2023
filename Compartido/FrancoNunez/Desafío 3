    async function convertToInteractive(key) {\n","          const element = document.querySelector('#df-3ab0b3a1-2c07-4a1b-b570-6092a46f205a');\n","          const dataTable =\n","            await google.colab.kernel.invokeFunction('convertToInteractive',\n","                                                     [key], {});\n","          if (!dataTable) return;\n","\n","          const docLinkHtml = 'Like what you see? Visit the ' +\n","            '<a target=\"_blank\" href=https://colab.research.google.com/notebooks/data_table.ipynb>data table notebook</a>'\n","            + ' to learn more about interactive tables.';\n","          element.innerHTML = '';\n","          dataTable['output_type'] = 'display_data';\n","          await google.colab.output.renderOutput(dataTable, element);\n","          const docLink = document.createElement('div');\n","          docLink.innerHTML = docLinkHtml;\n","          element.appendChild(docLink);\n","        }\n","      </script>\n","    </div>\n","  </div>\n","  "]},"metadata":{},"execution_count":38}]},{"cell_type":"code","source":["data.columns"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"NGr_re4fy67b","executionInfo":{"status":"ok","timestamp":1682264803653,"user_tz":180,"elapsed":367,"user":{"displayName":"Franco Nuñez","userId":"17725668663379934246"}},"outputId":"a321d2ff-9ee5-4022-9b77-7a42618e545d"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["Index(['Unnamed: 0', 'WKT', 'NOMBRE', 'CARACT_SIT', 'AUTORES', 'TECNICA',\n","       'F_E', 'TAMAÑO_TS', 'UBICACION', 'DIRECCION', 'DOM_ORIG', 'CALLE',\n","       'ALTURA', 'BARRIO', 'COMUNA'],\n","      dtype='object')"]},"metadata":{},"execution_count":39}]},{"cell_type":"code","source":["data['AUTORES']"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"Y_W_7qPjy9L6","executionInfo":{"status":"ok","timestamp":1682265114336,"user_tz":180,"elapsed":393,"user":{"displayName":"Franco Nuñez","userId"